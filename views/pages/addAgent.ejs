


    <%- include('../parts/head') %> 
    <%- include('../parts/menuinscription') %>

    
    
    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
        <div class="container" data-aos="fade-up">
  
          <br>
          <br>
          <br>
          <br>
          <br>
          <br>
          <div class="section-title">
            <h2>New Agent</h2>
            <p>Inscription :</p>
          </div>
  
  
          <div class="row mt-5">
  
            <!-- Form -->
            <div class="col-lg-8 mt-5 mt-lg-0 offset-lg-2">
  
              <!-- Form -->
              <form action="" method="post" >

                <div class="mb-3">
                  <div class="loading alert alert-info text-center"> <%= msg %> </div>
                </div>
               
                <div class="form-group">
                  <input required type="text" class="form-control" name="nom" id="subject" placeholder="Nom..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                    <input required type="text" class="form-control" name="prenom" id="subject" placeholder="Prenom..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                    <div class="validate"></div>
                </div>
                <div class="form-group">
                    <input required type="text" class="form-control" name="postnom" id="subject" placeholder="Postnom..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                    <div class="validate"></div>
                </div>
                <div class="form-group">
                  <input required type="email" class="form-control" name="email" id="subject" placeholder="Email..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                  <input required type="text" class="form-control" name="numTel" id="subject" placeholder="Number phone..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                    <input required type="password" class="form-control" name="passworld" id="subject" placeholder="Passworld..." data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                    <div class="validate"></div>
                </div>
                <div class="form-group">
                    <select required name="type" id="" class="form-control">
                      <option value="">Type d'Agent</option>
                        <% for(var i=0; i < type.length; i++){ %>
                            <option value="<%= type[i].type %>"> <%= type[i].type %>  </option>
                        <% } %>
                    </select><br>
                    <div class="validate"></div>
                </div>

                
                <div class="text-center"><button type="submit" class="btn btn-warning">Enregistrer</button></div>
              </form>
  
            </div>
  
          </div>
  
        </div>
    </section><!-- End Contact Section -->
  


    <%- include('../parts/footer') %>
