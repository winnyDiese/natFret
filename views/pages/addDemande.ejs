


    <%- include('../parts/head') %> 
    
    <% switch (type) {

      case "\"Client \"": %>
            <%- include('../parts/menuClient') %>
      <% break;

      case "\"Gerant \"": %>
        <%- include('../parts/menuGerant') %>
      <% break;

      case "\"Pointeur \"": %>
        <%- include('../parts/menuPointeur') %>
      <% break;

      default: %>
        <%- include('../parts/menuInscription') %>
      <% break;
  } %>

  
    
    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
        <div class="container" data-aos="fade-up">
  
          <br>
          <br>
          <br>
          <br>
          <br>
          <br>
          <div class="section-title">
            <h2>demande d'un depot</h2>
            <p>Nouvelle demande :</p>
          </div>
  
  
          <div class="row mt-5">
  
            <!-- Form -->
            <div class="col-lg-8 mt-5 mt-lg-0 offset-lg-2">
  
              <!-- Form -->
              <form action="" method="post">
                <div class="mb-3">
                
                  <div class="loading alert alert-info text-center"> <%= msg %> </div>
                </div>
                <div class="form-group">
                  <input required type="text" class="form-control" name="mse"  placeholder="Marchandise..." id="mse"/>
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                  <input required type="number" class="form-control" name="nbre"  placeholder="Nombre..." id="nbre" />
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                  <input required type="text" class="form-control" name="provenance"  placeholder="Prevenance..." id="provenance"/>
                  <div class="validate"></div>
                </div>
                <div class="form-group">
                  <input required type="text" class="form-control" name="destination"  placeholder="Destination..." id="destination" />
                  <div class="validate"></div>
                </div>
                
                <input type="hidden" name="idUser" id='idUser'>

                
                <div class="text-center"><button type="submit" class="btn btn-warning">Introduire</button></div>
              </form>

            </div>
  
          </div>
  
        </div>
      </section><!-- End Contact Section -->
  


    <%- include('../parts/footer') %>
    
